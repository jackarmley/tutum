header.site-header(role="banner" aria-label="Site header")
    .site-header-inner
        h1.site-logo: a(href='./')= locals.title
        nav.site-nav
            ul
                -each val, index in locals.nav
                    if(!val.private)
                        li: a(href=val.url data-title=val.title)= val.title
